<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="vo.Add_feedback_vo" table="td_feedback_dtl">
   
   	<id name="fb_id" type="long" column="ID" >
   		<generator class="increment"/>
  	</id>

  	<property name="fb_username">
   		<column name="Username" />
  	</property>
  
  	<property name="fb_msg">
  		<column name="Message"/>
  	</property>
  	  
 </class>

</hibernate-mapping>