<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
<session-factory>
  <property name="hibernate.connection.driver_class">com.mysql.cj.jdbc.Driver</property>
  <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/technocraft_debugger</property>
  <property name="hibernate.connection.username">root</property>
  <property name="hibernate.connection.password">mysql</property>
  <property name="hibernate.connection.pool_size">10</property>
  <property name="show_sql">true</property>
  <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
  <property name="hibernate.hbm2ddl.auto">update</property>
  <!-- Mapping files -->
  
  
  <mapping resource="user.hbm.xml"/>
  <mapping resource="vo/Add_cat.hbm.xml"/>
  <mapping resource="vo/Add_city.hbm.xml"/>
  <mapping resource="vo/Add_country.hbm.xml"/>
  <mapping resource="vo/Add_exam.hbm.xml"/>
  <mapping resource="vo/Add_feedback.hbm.xml"/>
  <mapping resource="vo/Add_package.hbm.xml"/>
  <mapping resource="vo/Add_qualification.hbm.xml"/>
  <mapping resource="vo/Add_questions.hbm.xml"/>
  <mapping resource="vo/Add_sub_cat.hbm.xml"/>  
  <mapping resource="vo/Add_job.hbm.xml"/>
  <mapping resource="vo/Add_state.hbm.xml"/>
  <mapping resource="vo/User_Feedback.hbm.xml"/>
  <mapping resource="vo/Company_Reg.hbm.xml"/>
  <mapping resource="vo/User_Reg.hbm.xml"/>
  <mapping resource="vo/user_mst.hbm.xml"/>
  <mapping resource="vo/Image_Upload.hbm.xml"/>
  <mapping resource="vo/Add_Event.hbm.xml"/>
  <mapping resource="vo/Apply_Job.hbm.xml"/>
  <mapping resource="vo/LinkVo.hbm.xml"/>
  <mapping resource="vo/Exam_Result.hbm.xml"/>
  
</session-factory>
</hibernate-configuration>